@model List<CallDto>

        <table class="table table-hover">
            <thead>
                <tr>
                    <td>Номер телефона</td>
                    <td>Время начала</td>
                    <td>Время кончания</td>
                </tr>
            </thead>
            <tbody>
                @{
                    if (Model.Count() != 0)
                    {
                        foreach (var call in Model)
                        {
                            if (call != null)
                            {
                                <tr>
                                    <td>@call.Number</td>
                                    <td>@call.StartTime</td>
                                    <td>@call.EndTime</td>
                                </tr>
                            }
                        }
                    }
                    else
                    {
                        <tr>
                            <td>Звонков нет</td>
                            <td>☻</td>
                            <td>☻</td>
                        </tr>
                    }
                }
            </tbody>
        </table>
